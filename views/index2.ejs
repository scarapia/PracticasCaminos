
<h3>Agregar nodo a  Station</h3>
<form method="post" action="/station/add">
    <label> Nombre  Nodo</label> <br>
    <input type="text" name = "station_name">
    <br>
    <label> Latitud</label> <br>
    <input type="number" name="station_latitude">
    <br>
    <label> Longitud</label><br>
    <input type="number" name="station_longitude">
    <br>
    <input type="submit" value="Crear Nodo">
</form>

<table border="1">    
        <tr>
            <th>Nombre</th>
                <% station.forEach(function(station){ %>
                <td>   <%= station.name %> </td>
                <% }) %>
        </tr>    
        <tr>
            <th>Latitud </th>
                <% station.forEach(function(station){ %>
                <td>   <%= station.latitude %> </td>
                <% }) %>
        </tr> 
        <tr>       
            <th>Longitud</th>
                <% station.forEach(function(station){ %>
                <td>   <%= station.longitude %> </td>
                <% }) %>
        </tr>        
</table>
<%- include('relconnection')%>;

<h3>Conectar nodos en  Station</h3>
<form method="post" action="/station/startfinish/add">
