    <br>
    <label> Nombre nodo destino</label> <br>
    <input type="text" name="stationFinish_name">
    <br>
    <label> Distancia</label><br>
    <input type="number" name="station_distance">
    <br>
    <input type="submit" value="Conectar Nodos">
</form>

